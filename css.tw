    35%  { left: 0.75em; opacity:1.0; filter:alpha(opacity=100); }
    100% { left: 0.35em; opacity:0.25; filter:alpha(opacity=25); }
 }
@-o-keyframes fade-bounce {
    0%   { left: 0; opacity:1.0; filter:alpha(opacity=100); }
    35%  { left: 0.75em; opacity:1.0; filter:alpha(opacity=100); }
    100% { left: 0.35em; opacity:0.25; filter:alpha(opacity=25); }
 }
@keyframes fade-bounce {
    0%   { left: 0; opacity:1.0; filter:alpha(opacity=100); }
    35%  { left: 0.75em; opacity:1.0; filter:alpha(opacity=100); }
