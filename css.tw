  background-color: #d9e3fa;
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #eef3fe), color-stop(100%, #d9e3fa));
  background-image: -webkit-linear-gradient(top, #eef3fe, #d9e3fa);
  background-image: -moz-linear-gradient(top, #eef3fe, #d9e3fa);
  background-image: -ms-linear-gradient(top, #eef3fe, #d9e3fa);
  background-image: -o-linear-gradient(top, #eef3fe, #d9e3fa);
  background-image: linear-gradient(top, #eef3fe, #d9e3fa);
  -webkit-box-shadow: inset 0 0 0 1px rgba(255, 255, 255, 0.9), 0 1px 2px rgba(0, 0, 0, 0.1);
  -moz-box-shadow: inset 0 0 0 1px rgba(255, 255, 255, 0.9), 0 1px 2px rgba(0, 0, 0, 0.1);
  box-shadow: inset 0 0 0 1px rgba(255, 255, 255, 0.9), 0 1px 2px rgba(0, 0, 0, 0.1);
}
.sf-menu::before, .sf-menu::after, .sf-menu li:first-child::after {
  display: block;
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  left: 5px;
  border: 7px outset transparent;
}
.sf-menu::before {
  border-bottom: 7px solid #555;
  top: -14px;
}
.sf-menu::after {
  border-bottom: 7px solid #EEF3FE;
  top: -12px;
}
.sf-menu li {
  /*padding: 0 12px;*/
  font-size: 11px;
  color: #838ca2;
  text-shadow: 0 1px #fff;
  white-space: nowrap; /* no need for Supersubs plugin */
  *white-space: normal; /* ...unless you support IE7 (let it wrap) */
  -webkit-transition: background .2s;
  transition: background .2s;
}
.sf-menu li.sep {
  border-bottom: 1px solid #b4bbce;
  padding-top:
  padding-bottom: 4px;
  margin-bottom: 4px;
  text-shadow:none;
}
.sf-menu li.sep:hover {
  border-color: #b4bbce;
  background: none;
  text-decoration: inherit;
  text-shadow: none;
}
.sf-menu a.chapter-list:hover::before {
  display:block;
}
.sf-menu a.chapter-list::before {
  display: none;
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  top: 50%;
  left: 2px;
  margin-top: -4px;
  border: 4px solid transparent;
  border-right-color: #9facd1;
}
.sf-menu a {
  display: block;
  position: relative;
  padding: 0 20px 0 12px;
  color: #313a4f;
  border: 1px solid transparent;
  text-decoration: none;
  zoom: 1; /* IE7 */
}
.sf-menu a.chapter-list {
  padding: 0 20px 0 15px;
}
.sf-menu a:hover {
  color: #13a;
  color: #fff;
}
.sf-menu li:hover,
.sf-menu li.sfHover {
  text-decoration: none;
  text-shadow: 0 1px rgba(0, 0, 0, 0.3);
  border-color: #5a70b0 #495d98 #42558a;
  background-color: #5a70b0;
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #7688bd), color-stop(100%, #4e64a3));
  background-image: -webkit-linear-gradient(top, #7688bd, #4e64a3);
  background-image: -moz-linear-gradient(top, #7688bd, #4e64a3);
  background-image: -ms-linear-gradient(top, #7688bd, #4e64a3);
  background-image: -o-linear-gradient(top, #7688bd, #4e64a3);
  background-image: linear-gradient(top, #7688bd, #4e64a3);
  /* only transition out, not in */
  -webkit-transition: none;
  transition: none;
}
.sf-menu  span.disable, .sf-menu span.disable:hover {
  color: #838ca2!important;
  padding: 6px 20px 0 12px;
}
.sf-menu ul {
  box-shadow: 2px 2px 6px rgba(0,0,0,.2);
  min-width: 12em; /* allow long menu items to determine submenu width */
  *width: 12em; /* no auto sub width for IE7, see white-space comment below */
}

.sf-menu ul li {
  background: #AABDE6;
}
.sf-menu ul ul li {
  background: #9AAEDB;
}

/* adding sf-vertical class in addition to sf-menu creates a vertical menu */
/* eg. <ul class="sf-menu sf-vertical"> ... */
.sf-vertical {
  min-width: 12em;
  *width: 12em;
}
.sf-vertical ul {
  right: 100%;
  top: 0;
}
.sf-vertical > li {
  float: none;
}
.sf-vertical li {
  width: 100%;
}



