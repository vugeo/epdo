:: Passage Transition CSS [stylesheet]
/* disable SugarCube's default passage transition */
.passage { transition: none; -webkit-transition: none; }

/* setup the new passage transition(s) */
/* this is a simple 3-step fade out 
.transition-in { opacity: 0; position: absolute; }
.passage:not(.transition-out) { transition: 0s 1100ms; -webkit-transition: 0s 1100ms; }
.transition-out { opacity:0; position: absolute; transition: 1000ms steps(3); -webkit-transition: 1000ms steps(3); }
*/

/* Fast scroll (normal flow) */

.transition-in:not(.flip) {
    opacity:0;
    transform: translate(0,-100rem);
    -webkit-transform: translate(0,-100rem);
    position:absolute;
}
.passage {
    transition: 0.7s;
    -webkit-transition: 0.7s;
}
.transition-out:not(.flip) {
    opacity:0;
    transform: translate(0,100rem);
    -webkit-transform: translate(0,100rem);
    position:absolute;
}


/* Page-flipping for sidebars */

.flip.transition-in {
    opacity:0;
    position:absolute;
    transform: rotateY(90deg);
    -webkit-transform: rotateY(90deg);    
}

.flip.transition-out {
    opacity:0;
    transform: rotateY(-90deg);
    -webkit-transform: rotateY(-90deg);
    position:absolute;
}

#passages {
    transition: background-color 0.4s cubic-bezier(0.95, 0.05, 0.795, 0.035);
    -webkit-transition: background-color 0.4s cubic-bezier(0.95, 0.05, 0.795, 0.035);
}


