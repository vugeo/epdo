:: Passage Transition CSS [stylesheet]
/* disable SugarCube's default passage transition */
.passage { transition: none; -webkit-transition: none; }

/* setup the new passage transition(s) */
/* this is a simple 3-step fade out 
.transition-in { opacity: 0; position: absolute; }
.passage:not(.transition-out) { transition: 0s 1100ms; -webkit-transition: 0s 1100ms; }
.transition-out { opacity:0; position: absolute; transition: 1000ms steps(3); -webkit-transition: 1000ms steps(3); }
*/

/* Fast scroll (normal flow) */

.transition-in:not(.flip) {
    opacity:0;
    transform: translate(0,-100rem);
    -webkit-transform: translate(0,-100rem);
    position:absolute;
}
.passage {
    transition: 0.7s;
    -webkit-transition: 0.7s;
}
.transition-out:not(.flip) {
    opacity:0;
    transform: translate(0,100rem);
    -webkit-transform: translate(0,100rem);
    position:absolute;
}


/* Page-flipping for sidebars */

.flip.transition-in {
    opacity:0;
    position:absolute;
    transform: rotateY(90deg);
    -webkit-transform: rotateY(90deg);    
}

.flip.transition-out {
    opacity:0;
    transform: rotateY(-90deg);
    -webkit-transform: rotateY(-90deg);
    position:absolute;
}

#passages {
    transition: background-color 0.4s cubic-bezier(0.95, 0.05, 0.795, 0.035);
    -webkit-transition: background-color 0.4s cubic-bezier(0.95, 0.05, 0.795, 0.035);
}


:: CSS for Sidebars [stylesheet]
#passages.sidebar {
    background-color: #FAF3E8;
}

.passage h1.sidebar {
    color:black;
}

.passage h4.sidebar {
    margin-bottom:0;
}

.passage h6.sidebar {
    margin:-0.4em 0 0 0;
}

.passage h6.sidebar a {
    color:#a8a8a8;
}

.passage hr.sidebar {
    background-color:black;
}

.sidebar-box {
  display:table;  
}

.sidebar-head {
    background-color:#dd3b2a;
    border:1px solid #dd3b2a;
    border-radius:5px 0 0 5px;
    color:white;
    padding:3px 10px;
    display:table-cell;
    vertical-align:middle;
    text-align:center;
    width:4em;
    line-height:140%;
}

.sidebar-container {
  margin:0;
}

ul.sidebar-links {
    border:1px solid #dd3b2a;
    border-radius:0 5px 5px 0;
    padding:3px 10px 3px 8px;
    line-height:100%;
    display:table-cell;
    vertical-align:middle;
    list-style-type: none;
}

.sidebar-links li {
  margin:9px 0;
}

.sidebar-links a, sidebar-link a:hover {
    text-decoration:underline;
    color:#dd3b2a;
}


:: CSS for circle graphs [stylesheet]
.circle-graph {
  width: 30px;
  height: 30px;
  -moz-border-radius: 50%;
  -webkit-border-radius: 50%;
  border-radius: 50%;
  display:inline-block;
}

.circle-graph-full {
    background: #0688fa;
}

.circle-graph-empty {
    background: #bdbdbd;
}

.circle-graph-clip-container {
    position:relative;
    width:30px;
    height:30px;
    display:inline-block;
}

.circle-graph-clip {
    position:absolute;
    top:0;
    left:0;
}


:: CSS for replacelinks macros [stylesheet]
.revision-span-in { opacity: 0; }
.revision-span:not(.revision-span-out) { transition: 0.4s; -webkit-transition: 0.4s; }
.revision-span-out { position: absolute; opacity: 0; }

