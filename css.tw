/* CSS3 calc() support check */
#css3-calc {
    width: 10px;
    width: calc(10px + 10px);
    display: none;
}

html {
  height:100%;
}
figcaption {
    font-size:80%;
    font-style:italic;
}
.inline {
    display: inline-block;
}
.inline-head {
    font-weight:bold;
}
.left {
    float: left;
}
.right {
    float:right;
}
.talkbox {
  width:85%;
  margin:auto;
}
body {
    color: #000;
    font-family: 'Open Sans',serif;
    margin:0;
    height:100%;
    overflow:auto;
    position:relative;
    z-index: 3;
}
h1 {
    font-family: 'Arvo',serif;
    font-weight:700;
    font-size: 36px;
    line-height: 1.2em;
    margin: 0 0 10px 0;
    color: #dd3b2a;
}
h4 {
    font-family: 'Arvo',serif;
    font-weight:700;
    font-size:18px;
    margin-top:0;
}
h6 {
    font-family: 'Arvo',serif;
    font-weight:700;
    font-size:14px;
    margin:0;
}
.img-container {
    position:fixed;
    top:0;
    left:0;
    background-repeat: no-repeat;
    background-position: center center;
    background-attachment: fixed; 
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    height:100%;
    width:100%;
    -webkit-transition: opacity 1s ease-in-out;
    -moz-transition: opacity 1s ease-in-out;
    -o-transition: opacity 1s ease-in-out;
    transition: opacity 1s ease-in-out;
}
.img-anchor {
    opacity:1;
    z-index:1;
}
.img-fader {
    z-index:2;
    
}
.background-fade {
    opacity:0;
}


/* Make link font slightly smaller to compensate for boldface */
.content a.link-internal,
.content a.link-external,
.content a.link-back,
.content a.link-return {
    font-size: 90%;
}
.content strong {
    font-size: 90%;
}

/* Basic passage styles */
#passages {
    color: #000;
    font-size:15px;
    background-color:#fefefe;
    
    min-height:100%;
    margin: 0 auto;
    border-left:none;
    padding: 55px 2em 3em 2em;
    -moz-box-shadow: 0 0 20px black;
    -webkit-box-shadow: 0 0 20px black;
    box-shadow: 0 0 20px black;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing:border-box;
    position:relative;
}
@media screen {
  #passages {
    width:60%;
  }
}

@media only screen and (max-width : 999px) { 
  #passages {
    width:100%;
    padding:55px 1em 3em 1em;
  }
  #custom {
    width:100%!important;
    margin:0!important;
  }
  #backgrounds {
    display:none;
  }
  .passage h1 {
    font-size:30px;
  }
  .passage h4 {
    font-size:16px;
  }
  .passage table {
    margin:0.8em 0;
  }
  #story-rewind::before {
    content:""!important;
  }
  #sound-controls::before {
    content:"Sound"!important;
  }
  h1.index-section {
    margin-top:1em;
  }
  .index-columns p {
  line-height: 120%;
}
}

@media print {
  #passages {
    width:100%;
  }
}
.passage {
    line-height: 1.75em;
    margin-bottom: 0;
    text-align: left;
    height:100%;
}
#ui-bar {
    display:none;
}
#passages table thead td {
    font-weight:bold;
}
#passages table.alternating-rows tr:nth-child(odd) {
    background-color:#EEEEEE;/*Fallback*/
    background-color:rgba(221,221,221,0.5);
}
#passages table.alternating-rows tr:nth-child(even) {
    background-color:#DDDDDD;/*Fallback*/
    background-color:rgba(204,204,204,0.5);
}
#passages table.box {
    background-color:#EEEEEE;/*Fallback*/
    background-color:rgba(221,221,221,0.5);
}
#passages table.box th, #passages table.box thead {
    background-color:#DDDDDD;/*Fallback*/
    background-color:rgba(204,204,204,0.5);
}
#passages table.box tr:first-child td,
#passages table.box tr:first-child th,
#passages table.box th tr:first-child td {
    padding-top:10px;
}
#passages table.box tr:last-child td {
    padding-bottom:10px;
}
#passages table {
    font-size: 80%;
    border-collapse: collapse;
    line-height:145%;
}
#passages th, #passages td {
    padding: 0.5em 1em;
}
#passages table a.link-internal, #passages table a.link-external, #passages table a.link-back, #passages table a.link-return {
    font-size: 100%!important;
}
#passages table strong {
    font-size: 100%!important;
}
table li {
  border-radius:50%;
  width:1.4em;
  height:1.4em;
  display:inline-block;
  text-align:center;
  font-weight:bold;
  text-decoration:none;
  padding-left: 1px;
  padding-bottom: 1px;
  color: #ffffff;
  background-color: #555555;
}


/* Figures */
figure {
  margin-left:auto;
  margin-right:auto;
  display:table;
}

/* Special layout options for full presentation print version */
.full-print {
  margin-top: 30px;
}

h1.full-print {
  color: black!important;
  font-size: 42px;
  border-top: 2px black solid!important;
  margin-top: 40px;
  padding-top: 10px;
  border-bottom: 2px black solid!important;
  padding-bottom: 10px;
  background-color: beige!important;
}

@media print {
  .full-print {
    page-break-after:always;
  }
}

/* Multicolumn layout for index */
.index-columns {
  -webkit-column-count: 2;
  -webkit-column-gap: 30px; 
  -webkit-column-rule: 0px dotted black;
  -webkit-column-fill: balance;
  -moz-column-count: 2;
  -moz-column-gap: 30px;  
  -moz-column-rule: 0px dotted black;
  -moz-column-fill: balance;
  column-count: 2;
  column-gap: 30px; 
  column-rule: 0px dotted black;
  column-fill: balance;
  /*display: inline-block;*/ /*needed for webkit to balance columns properly */
}

.index-item {
  display: inline-block;
}

h1.index-section {
  font-size: 18px;
  color: black;
  margin-top:0.5em;
}

.index-columns p {
  -webkit-margin-before: 0;
  line-height: 100%;
}

p.index-sidebar {
  margin-left: 2em;
  font-size: 80%;
}


/*Admin area */
