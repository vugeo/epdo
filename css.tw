        rRadial = /^(.*?)radial-gradient(.*?)$/i,
        rLinearSettings = /^(.*?)(:?linear-gradient)(\()(.*)(\))(.*?)$/i,       
        rRadialSettings = /^(.*?)(:?radial-gradient)(\()(.*?)(\))(.*?)$/i,
        rSupportLinearW3C = /(^|\s)linear-gradient/,    
