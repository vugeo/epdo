}


:: Help system [stylesheet]
.help-box {
  -moz-box-sizing: border-box;
       box-sizing: border-box;
  background: #ffce0d;
  color: #000;
  display: block;
  opacity: 0;
  filter: alpha(opacity=0);
  padding: 20px;
  pointer-events: none;
  position: absolute;
  width: 200px;
  z-index: 10000;
  border-radius: 10px;
  -webkit-transform: translateY(10px);
     -moz-transform: translateY(10px);
       -o-transform: translateY(10px);
          transform: translateY(10px);
  -webkit-transition: opacity .25s ease-out, -webkit-transform .25s ease-out;
     -moz-transition: opacity .25s ease-out, -moz-transform .25s ease-out;
       -o-transition: opacity .25s ease-out, -o-transform .25s ease-out;
          transition: opacity .25s ease-out, transform .25s ease-out;
  box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
}

.help-box .arrow {
  position: absolute;
  border-width: 10px;
  border-style: solid;
  height: 0;
  width: 0;
  border-color: #ffce0d;
}

.arrow.arrow-down {
  left: 50%;
  top: 100%;
  border-right-color: transparent;
  border-left-color: transparent;
  border-bottom-color: transparent;
  margin-left: -10px;
}

.arrow.arrow-up {
  left: 50%;
  top: -20px;
  border-right-color: transparent;
  border-left-color: transparent;
  border-top-color: transparent;
  margin-left: -10px;
}

.help-box-show {
  opacity: 1;
  filter: alpha(opacity=100);
  pointer-events: auto;
  -webkit-transform: translateY(0px);
     -moz-transform: translateY(0px);
       -o-transform: translateY(0px);
          transform: translateY(0px);
}

.help-box h1 {
  font-size: 13px;
  color: #000;
}

.help-box hr {
  background: #fff;
  height: 1px;
  border: none;
}

.help-box p {
  font-size: 12px;
}

.help-box-callout {
  box-shadow: 0px 0px 6px 6px #fff!important;
  border-radius: none;
  background-color: #fff;
}

.help-box-callout.help-box-callout-alt {
  box-shadow: 0px 0px 6px 6px yellow!important;
  background-color: yellow;
}

.help-box-nav {
  height: 16px;
  display: block;
}

a.help-box-close {
  display: block;
  position: absolute;
  top: 5px;
  right: 8px;
  width: 12px;
  height: 12px;
}

svg.help-box-close {
  fill: #666;
  width: 12px;
  height: 12px;
}

a.help-box-next {
  display: block;
  right: 20px;
  bottom: 10px;
  position: absolute;
  font-weight: bold;
  color: #fff;
}

svg.help-box-next {
  fill: #fff;
}

a.help-box-prev {
  display: block;
  left: 20px;
  bottom: 10px;
  position: absolute;
}

svg.help-box-prev {
  fill: #fff;
}
.help-external-link-icon:before {
  font-family: 'icomoon-sugarcube';
  content: "\00a0\e010";
}
ul.passage-list li {
  position: relative;
}
